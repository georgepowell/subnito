{
  "name": "Autocognito for Reddit",
  "version": "0.1",
  "description": "Automatically opens NSFW subreddits in incognito mode",
  "background": {
    "scripts": ["jquery.js", "background.js"]
  },
  "permissions" : [
    "tabs",
    "history",
    "webNavigation"
  ],
  "icons" : {
    "19" : "icon-19.png",
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 2
}
